SELECT COUNT(*) AS tablas_nuam
FROM user_tables
WHERE table_name IN (
  'PAIS','MONEDA','MONEDA_PAIS','PERSONA','USUARIO','ROL','USUARIO_ROL',
  'MERCADO','CORREDORA','CORREDORA_IDENTIFICADOR','USUARIO_CORREDORA',
  'FUENTE','INSTRUMENTO','EVENTO_CAPITAL','FACTOR_DEF','CALIFICACION',
  'CALIFICACION_MONTO_DETALLE','CALIFICACION_FACTOR_DETALLE',
  'CARGA','CARGA_DETALLE','HOMOLOGACION_CAMPO','AUDITORIA','COLABORADOR'
);
